{
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rulesDirectory": [
    ],
    "rules": {
        "semicolon": true,
        "import-spacing": true,
        "quotemark": true,
        "whitespace": true,
        "no-debugger": true,
        "eofline": true,
        "no-console": true,
        "prefer-const": true,
        "no-unused-expression": true,
        "interface-name": false,
        "ordered-imports": false,
        "member-ordering": false,
        "object-literal-sort-keys": false,
        "member-access": false,
        "jsx-no-multiline-js": false
    }
}